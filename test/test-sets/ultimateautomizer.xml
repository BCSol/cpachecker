<?xml version="1.0"?>
<!DOCTYPE benchmark SYSTEM "benchmark.dtd">
<benchmark tool="ultimateautomizer" timelimit="900" hardtimelimit="930" memlimit="15000" cpuCores="8">

  <require cpuCores="8"/>
  <resultfiles>**.*</resultfiles>
  <propertyfile>${sourcefile_path}/ALL.prp</propertyfile>
    
  <option>32bit</option>
  <option>precise</option>
    
    <propertyfile>../programs/benchmarks/recursive-simple/ALL.prp</propertyfile>
  
    <sourcefiles name="simple">
      <include>../programs/benchmarks/recursive-simple/*.c</include>
    </sourcefiles>
    
    <sourcefiles name="sv">
      <includesfile>../programs/benchmarks/Recursive.set</includesfile>
    </sourcefiles>
  
  <rundefinition name="ultimateautomizer-recursive"></rundefinition>

</benchmark>
