<?xml version="1.0"?>
<!DOCTYPE benchmark SYSTEM "benchmark.dtd">
<!-- <benchmark tool="cpatiger-to" timelimit="930" hardtimelimit="1030" memlimit="6000" cpuCores="2"> -->
<benchmark tool="cpatiger-to" timelimit="930">

  <option name="-heap">4800M</option>


  <sourcefiles name="ntdrivers-simplified">
    <includesfile>../../../programs/fql/tiger-sets/cf/ssh-simplified.set</includesfile>
  </sourcefiles>

  <rundefinition name="predicate-bb-summaries">
	<option name="-noout"/>
	<option name="-disable-java-assertions"/>
	<option name="-stats"/>
	<option name="-bb2"/>
	<option name="-pred"/>
	<option name="-pedantic"/>
	<option name="-summaries"/>
	<option name="-forceto">900</option> <!-- interrupts the process after the time, before it gets killed -->
  </rundefinition>

  <rundefinition name="predicate-bb">
	<option name="-noout"/>
  	<option name="-disable-java-assertions"/>
  	<option name="-stats"/>
  	<option name="-bb2"/>
  	<option name="-pred"/>
  	<option name="-pedantic"/>
  	<option name="-forceto">900</option>
  </rundefinition>


  <columns>
    <column title="time in reach">Time in reach</column>
    <column title="goals">#Goals</column>
    <column title="feasible">#Feasible</column>
    <column title="infeasible">#Infeasible</column>
    <column title="imprecise">#Imprecise</column>
    <column title="bugrevealing">#BugRevealing</column>
    <column title="summary cov. rate">Summary cov. successfull</column>
    <column title="summary cov. time">Time for summary check</column>
    <column title="summary size">Summary size</column>
  </columns>



</benchmark>
