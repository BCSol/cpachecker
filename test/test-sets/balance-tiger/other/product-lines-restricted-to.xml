<?xml version="1.0"?>
<!DOCTYPE benchmark SYSTEM "benchmark.dtd">
<benchmark tool="cpatiger-to" timelimit="930" hardtimelimit="1030" memlimit="6000" cpuCores="2">
  <option name="-heap">4800M</option>

  <sourcefiles name="product-lines">
    <includesfile>../../programs/fql/tiger-sets/cf/productlines.set</includesfile>
	<exclude>../../programs/benchmarks/product-lines/email_spec1_product16_false-unreach-label.cil.c</exclude>
	<exclude>../../programs/benchmarks/product-lines/email_spec6_product12_false-unreach-label.cil.c</exclude>
	<exclude>../../programs/benchmarks/product-lines/email_spec0_product37_true-unreach-label.cil.c</exclude>
	<exclude>../../programs/benchmarks/product-lines/email_spec0_product11_true-unreach-label.cil.c</exclude>
	<exclude>../../programs/benchmarks/product-lines/email_spec0_product38_true-unreach-label.cil.c</exclude>
	<exclude>../../programs/benchmarks/product-lines/email_spec0_product40_true-unreach-label.cil.c</exclude>
	<exclude>../../programs/benchmarks/product-lines/email_spec11_product37_true-unreach-label.cil.c</exclude>
	<exclude>../../programs/benchmarks/product-lines/email_spec11_product39_true-unreach-label.cil.c</exclude>
	<exclude>../../programs/benchmarks/product-lines/email_spec11_product40_true-unreach-label.cil.c</exclude>
	<exclude>../../programs/benchmarks/product-lines/email_spec1_product12_true-unreach-label.cil.c</exclude>
	<exclude>../../programs/benchmarks/product-lines/email_spec8_product14_true-unreach-label.cil.c</exclude>
	<exclude>../../programs/benchmarks/product-lines/email_spec9_product12_true-unreach-label.cil.c</exclude>
  </sourcefiles>

  <rundefinition name="predicate-bb">
	<option name="-bb"/>
	<option name="-pred"/>
	<option name="-pedantic"/>
	<option name="-forceto">900</option> <!-- interrupts the process after the time, before it gets killed -->
  </rundefinition>

  <rundefinition name="explicit-sim-bb">
        <option name="-bb"/>
        <option name="-expsim"/>
        <option name="-pedantic"/>
	<option name="-forceto">900</option> <!-- interrupts the process after the time, before it gets killed -->
  </rundefinition>

  <rundefinition name="explicit-ref-bb">
        <option name="-bb"/>
        <option name="-expref"/>
        <option name="-pedantic"/>
<option name="-forceto">900</option> <!-- interrupts the process after the time, before it gets killed -->
  </rundefinition>

  <rundefinition name="explicit-pred-bb">
        <option name="-bb"/>
        <option name="-exppred"/>
        <option name="-pedantic"/>
	<option name="-forceto">900</option> <!-- interrupts the process after the time, before it gets killed -->
  </rundefinition>

  <columns>
    <column title="time in reach">Time in reach</column>
    <column title="goals">#Goals</column>
    <column title="feasible">#Feasible</column>
    <column title="infeasible">#Infeasible</column>
    <column title="imprecise">#Imprecise</column>
    <column title="bugrevealing">#BugRevealing</column>
#Goals: 
  </columns>
</benchmark>
