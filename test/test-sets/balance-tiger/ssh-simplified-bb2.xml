<?xml version="1.0"?>
<!DOCTYPE benchmark SYSTEM "benchmark.dtd">
<benchmark tool="cpatiger" timelimit="900" hardtimelimit="1000" memlimit="6000" cpuCores="2">
  <option name="-heap">4800M</option>


  <sourcefiles name="ssh-simplified">
    <includesfile>../../programs/fql/tiger-sets/cf/ssh-simplified.set</includesfile>
  </sourcefiles>

  <rundefinition name="predicate-bb2">
	<option name="-bb2"/>
	<option name="-pred"/>
	<option name="-pedantic"/>
  </rundefinition>

  <rundefinition name="explicit-sim-bb2">
        <option name="-bb2"/>
        <option name="-expsim"/>
        <option name="-pedantic"/>
  </rundefinition>

  <rundefinition name="explicit-ref-bb2">
        <option name="-bb2"/>
        <option name="-expref"/>
        <option name="-pedantic"/>
  </rundefinition>

  <rundefinition name="explicit-pred-bb2">
        <option name="-bb2"/>
        <option name="-exppred"/>
        <option name="-pedantic"/>
  </rundefinition>
  
  <columns>
    <column title="time in reach">Time in reach</column>
    <column title="goals">#Goals</column>
    <column title="feasible">#Feasible</column>
    <column title="infeasible">#Infeasible</column>
    <column title="imprecise">#Imprecise</column>
    <column title="bugrevealing">#BugRevealing</column>
#Goals: 
  </columns>
</benchmark>
