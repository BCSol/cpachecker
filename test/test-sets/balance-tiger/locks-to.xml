<?xml version="1.0"?>
<!DOCTYPE benchmark SYSTEM "benchmark.dtd">
<benchmark tool="cpatiger-to" timelimit="915" hardtimelimit="1015" memlimit="6000" cpuCores="2">
  <option name="-heap">4800M</option>

  <sourcefiles name="locks">
    <includesfile>../../programs/fql/tiger-sets/cf/locks.set</includesfile>
  </sourcefiles>

  <rundefinition name="predicate-bb">
	<option name="-bb"/>
	<option name="-pred"/>
	<option name="-pedantic"/>
	<option name="-forceto">900</option> <!-- interrupts the process after the time, before it gets killed -->
  </rundefinition>

  <rundefinition name="explicit-sim-bb">
        <option name="-bb"/>
        <option name="-expsim"/>
        <option name="-pedantic"/>
	<option name="-forceto">900</option> <!-- interrupts the process after the time, before it gets killed -->
  </rundefinition>

  <rundefinition name="explicit-ref-bb">
        <option name="-bb"/>
        <option name="-expref"/>
        <option name="-pedantic"/>
<option name="-forceto">900</option> <!-- interrupts the process after the time, before it gets killed -->
  </rundefinition>

  <rundefinition name="explicit-pred-bb">
        <option name="-bb"/>
        <option name="-exppred"/>
        <option name="-pedantic"/>
	<option name="-forceto">900</option> <!-- interrupts the process after the time, before it gets killed -->
  </rundefinition>

  <columns>
    <column title="time in reach">Time in reach</column>
    <column title="goals">#Goals</column>
    <column title="feasible">#Feasible</column>
    <column title="infeasible">#Infeasible</column>
    <column title="imprecise">#Imprecise</column>
    <column title="bugrevealing">#BugRevealing</column>
#Goals: 
  </columns>
</benchmark>
