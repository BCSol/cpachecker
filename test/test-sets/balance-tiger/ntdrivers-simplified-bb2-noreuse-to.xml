<?xml version="1.0"?>
<!DOCTYPE benchmark SYSTEM "benchmark.dtd">
<benchmark tool="cpatiger-to" timelimit="930" hardtimelimit="1030" memlimit="6000" cpuCores="2">
  <option name="-heap">4800M</option>


  <sourcefiles name="ntdrivers-simplified-noreuse">
    <includesfile>../../programs/fql/tiger-sets/cf/ntdrivers-simplified.set</includesfile>
  </sourcefiles>

  <rundefinition name="predicate-bb2">
	<option name="-noreuse"/>
	<option name="-noout"/>
	<option name="-disable-java-assertions"/>
	<option name="-stats"/>
	<option name="-bb2"/>
	<option name="-pred"/>
	<option name="-pedantic"/>
	<option name="-forceto">900</option> <!-- interrupts the process after the time, before it gets killed -->
  </rundefinition>

  <rundefinition name="explicit-sim-bb2">
	<option name="-noreuse"/>
	<option name="-noout"/>
	<option name="-disable-java-assertions"/>
	<option name="-stats"/>
	<option name="-rtimelimit">30</option>
        <option name="-bb2"/>
        <option name="-expsim"/>
        <option name="-pedantic"/>
	<option name="-forceto">900</option> <!-- interrupts the process after the time, before it gets killed -->
  </rundefinition>

  <rundefinition name="explicit-ref-bb2">
	<option name="-noreuse"/>
	<option name="-noout"/>
	<option name="-disable-java-assertions"/>
	<option name="-stats"/>
	<option name="-rtimelimit">30</option>
        <option name="-bb2"/>
        <option name="-expref"/>
        <option name="-pedantic"/>
<option name="-forceto">900</option> <!-- interrupts the process after the time, before it gets killed -->
  </rundefinition>

  <rundefinition name="explicit-pred-bb2">
	<option name="-noreuse"/>
	<option name="-noout"/>
	<option name="-disable-java-assertions"/>
	<option name="-stats"/>
	<option name="-rtimelimit">30</option>
        <option name="-bb2"/>
        <option name="-exppred"/>
        <option name="-pedantic"/>
	<option name="-forceto">900</option> <!-- interrupts the process after the time, before it gets killed -->
  </rundefinition>

  <columns>
    <column title="time in reach">Time in reach</column>
    <column title="goals">#Goals</column>
    <column title="feasible">#Feasible</column>
    <column title="infeasible">#Infeasible</column>
    <column title="imprecise">#Imprecise</column>
    <column title="bugrevealing">#BugRevealing</column>

  </columns>
</benchmark>
