<?xml version="1.0"?>
<!DOCTYPE benchmark SYSTEM "benchmark.dtd">
<benchmark tool="cpatiger" timelimit="30">
  <option name="-heap">1200M</option>


  <sourcefiles name="ntdrivers-simplified">
    <includesfile>../programs/fql/ntdrivers-simplified.set</includesfile>
  </sourcefiles>

  <rundefinition name="explicit-simple-bb">
	<option name="-bb"/>
	<option name="-expsim"/>
  </rundefinition>

  <columns>
    <column title="time in reach">Time in reach</column>
    <column title="goals">#Goals</column>
    <column title="feasible">#Feasible</column>
    <column title="infeasible">#Infeasible</column>
    <column title="imprecise">#Imprecise</column>
#Goals: 
  </columns>
</benchmark>
