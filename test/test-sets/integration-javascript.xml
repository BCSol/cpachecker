<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 1.0//EN" "http://www.sosy-lab.org/benchexec/benchmark-1.0.dtd">
<benchmark tool="cpachecker" timelimit="20 s" hardtimelimit="30 s" memlimit="500 MB" cpuCores="1">

  <option name="-noout"/>
  <option name="-heap">150M</option>
  <option name="-setprop">language=JavaScript</option>
  <option name="-generateCFA"/>

  <rundefinition/>

  <tasks>
    <include>../programs/javascript/**.js</include>
  </tasks>
</benchmark>
