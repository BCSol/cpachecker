unsigned get_initial_state() {
	return 0;
}

unsigned compute_successor(unsigned state, std::string op) {
	if(op == "ssl3_get_client_hello") {
		if(state == 0) {
			state = 1;
		}
	}
	else if(op == "ssl3_send_server_hello") {
		if(state == 1) {
			state = 2;
		}
	}	
	else if(op == "ssl3_send_server_certificate") {
		if(state == 2) {
			state = 3;
		}
	}
	else if(op == "ssl3_send_server_key_exchange") {
		if(state == 3) {
			state = 4;
		}
	}
	else if(op == "ssl3_send_certificate_request") {
		if(state == 4) {
			state = 5;	
		}
	}
	else if(op == "ssl3_check_client_hello") {
		if(state == 5) {
			state = 6;			
		}
	}
	else if(op == "ssl3_get_client_certificate") {
		if(state == 6) {
			state = 7;
		}
	}
	else if(op == "ssl3_get_client_key_exchange") {
		if(state == 7) {
			state = 8;
		}
	}
	else if(op == "ssl3_get_cert_verify") {
		if(state == 8) {
			state = 9;
		}
	}
	else if(op == "ssl3_get_finished") {
		if(state == 9) {
			state = 10;
		}
		else if(state == 12) {
			state = 13;
		}
		else if(state == 15) {
			state = 16;
		}
		else if(state == 18) {
			state = 19;
		}
		else if(state == 21) {
			printf("ERROR: error location is reachable\n");
			exit(EXIT_FAILURE);
		}
	}
	else if(op == "ssl3_send_change_cipher_spec") {
		if(state == 10) {
			state = 11;
		}
		else if(state == 13) {
			state = 14;
		}
		else if(state == 16) {
			state = 17;
		}
		else if(state == 19) {
			state = 20;
		}
	}
	else if(op == "ssl3_send_finished") {
		if(state == 11) {
			state = 12;
		}
		else if(state == 14) {
			state = 15;
		}
		else if(state == 17) {
			state = 18;
		}
		else if(state == 20) {
			state = 21;
		}
	}
	return state;
}
