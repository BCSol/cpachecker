unsigned get_initial_state() {
	return 0;
}

unsigned compute_successor(unsigned state, std::string op) {
	if(op == "ssl3_client_hello") {
		if(state == 0) {
			state = 1;
		}
	}
	else if(op == "ssl3_get_server_hello") {
		if(state == 1) {
			state = 2;
		}
	}
	else if(op == "ssl3_get_server_certificate") {
		if(state == 2) {
			state = 3;
		}
	}
	else if(op == "ssl3_get_key_exchange") {
		if(state == 3) {
			state = 4;
		}
	}
	else if(op == "ssl3_get_certificate_request") {
		if(state == 5) {
			printf("ERROR: error location is reachable\n");
			exit(EXIT_FAILURE);		
		}
	} else if (op == "ssl3_get_server_done") {
		if (state == 4) {
			state = 5;
		}
	}
	return state;
}


