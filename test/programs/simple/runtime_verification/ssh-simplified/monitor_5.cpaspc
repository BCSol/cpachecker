OBSERVER AUTOMATON Monitor5Automaton

INITIAL STATE Init;

STATE USEFIRST Init :
  MATCH  {$? = ssl3_get_client_hello();}  -> GOTO STATE1;

STATE USEFIRST STATE1 :
  MATCH  {$? = ssl3_send_server_hello();}  -> GOTO STATE2;

STATE USEFIRST STATE2 :
  MATCH  {$? = ssl3_send_server_certificate();}  -> GOTO STATE6;
  MATCH  {$? = ssl3_send_change_cipher_spec();}  -> GOTO STATE3;

STATE USEFIRST STATE3 :
  MATCH  {$? = ssl3_send_finished();}  -> GOTO STATE4;

STATE USEFIRST STATE4 :
  MATCH  {$? = ssl3_send_change_cipher_spec();}  -> GOTO STATE5;

STATE USEFIRST STATE5 :
  MATCH  {$? = ssl3_get_finished();}  -> ERROR;

STATE USEFIRST STATE6 :
  MATCH  {$? = ssl3_send_server_key_exchange();}  -> GOTO STATE7;

STATE USEFIRST STATE7 :
  MATCH  {$? = ssl3_send_change_cipher_spec();}  -> GOTO STATE8;

STATE USEFIRST STATE8 :
  MATCH  {$? = ssl3_send_certificate_request();}  -> ERROR;

END AUTOMATON
